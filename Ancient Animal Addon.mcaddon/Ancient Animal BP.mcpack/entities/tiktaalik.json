{
  "format_version": "1.13.0",
  "minecraft:entity": {
    "description": {
      "identifier": "hachchch:tiktaalik",
      "is_spawnable": true,
      "is_summonable": true,
      "is_experimental": false
    },
    "component_groups": {
      "hachchch:tiktaalik_baby": {
        "minecraft:is_baby": {
        },
        "minecraft:scale": {
          "value":0.5
        },
        "minecraft:ageable": {
          "duration": 1200,
          "feed_items": ["cod","salmon","tropical_fish"],
          "grow_up": {
            "event": "minecraft:ageable_grow_up",
            "target": "self"
          }
        },

        "minecraft:behavior.follow_parent": {
          "priority": 6,
          "speed_multiplier": 1.1
        }
      },

      "hachchch:tiktaalik_adult": {
        "minecraft:experience_reward": {
          "on_bred": "Math.Random(1,7)",
          "on_death": "query.last_hit_by_player ? Math.Random(1,3) : 0"
        },
        "minecraft:loot": {
          "table": "loot_tables/entities/cod.json"
        },
        "minecraft:behavior.breed": {
          "priority": 3,
          "speed_multiplier": 1.0
        },
        "minecraft:breedable": {
          "require_tame": false,
          "breed_items": ["cod","salmon","tropical_fish"],
          "breeds_with": {
            "mate_type": "hachchch:tiktaalik",
            "baby_type": "hachchch:tiktaalik",
            "breed_event": {
              "event": "minecraft:entity_born",
              "target": "baby"
            }
          }
        }
      }
    },

    "components": {
      "minecraft:type_family": {
        "family": ["sarcopterygii","tiktaalik","mob"]
      },
      "minecraft:breathable": {
        "breathes_air" : true,
        "breathes_water" : true,
        "total_supply": 15,
        "suffocate_time": 0
      },
      "minecraft:navigation.generic": {
        "can_walk": true,
        "can_swim": true,
        "can_path_over_water": true,
        "can_sink": false,
        "can_breach": true,
        "avoid_damage_blocks": true
      },
      "minecraft:movement.basic": {

      },
      "minecraft:jump.static": {
      },
      "minecraft:can_climb": {
      },
      "minecraft:collision_box": {
        "width": 0.3,
        "height": 0.2
      },
      "minecraft:nameable": {
      },
      "minecraft:health": {
        "value": 10,
        "max": 10
      },
      "minecraft:hurt_on_condition": {
        "damage_conditions": [
          {
            "filters": { "test": "in_lava", "subject": "self", "operator": "==", "value": true },
            "cause": "lava",
            "damage_per_tick": 4
          }
        ]
      },
      "minecraft:movement": {
        "value": 0.10
      },
      "minecraft:underwater_movement": {
        "value": 0.25
      },
      "minecraft:behavior.breed": {
        "priority": 3,
        "speed_multiplier": 1.0
      },
      "minecraft:behavior.tempt": {
        "priority": 4,
        "speed_multiplier": 1.25,
        "items": [
          "cod","salmon","tropical_fish"
        ]
      },
      "minecraft:behavior.follow_parent": {
        "priority": 5,
        "speed_multiplier": 1.1
      },
      "minecraft:behavior.random_stroll": {
        "priority": 6,
        "speed_multiplier": 0.8
      },
      "minecraft:behavior.look_at_player": {
        "priority": 7,
        "look_distance": 6.0,
        "probability": 0.02
      },
      "minecraft:behavior.random_look_around": {
        "priority": 9
      },
      "minecraft:balloonable": {
      },
      "minecraft:physics": {
      },
      "minecraft:pushable": {
        "is_pushable": true,
        "is_pushable_by_piston": true
      }
    },

    "events": {
      "minecraft:entity_spawned": {
        "randomize": [
          {
            "weight": 95,
            "add": {
              "component_groups": [
                "hachchch:tiktaalik_adult"
              ]
            }
          },
          {
            "weight": 5,
            "add": {
              "component_groups": [
                "hachchch:tiktaalik_baby"
              ]
            }
          }
        ]
      },

      "minecraft:entity_born": {
        "add": {
          "component_groups": [
            "hachchch:tiktaalik_baby"
          ]
        }
      },

      "minecraft:entity_transformed": {
        "remove": {
        },
        "add": {
          "component_groups": [
            "hachchch:tiktaalik_adult"
          ]
        }
      },

      "minecraft:ageable_grow_up": {
        "remove": {
          "component_groups": [
            "hachchch:tiktaalik_baby"
          ]
        },
        "add": {
          "component_groups": [
            "hachchch:tiktaalik_adult"
          ]
        }
